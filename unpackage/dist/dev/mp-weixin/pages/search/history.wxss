.search-history {
  padding: 20rpx 48rpx;
  display: flex;
  column-gap: 20rpx;
  align-items: center;
  box-sizing: border-box;
}
.search-history-tagsbox {
  overflow: hidden;
  position: relative;
  flex: 1;
}
.search-history-tags {
  display: flex;
  flex-wrap: nowrap;
  column-gap: 20rpx;
  overflow-x: scroll;
}
.search-history-tags::-webkit-scrollbar {
  display: none;
}
.search-history-tags .search-history-tag {
  padding: 12rpx 24rpx;
  background-color: #f2f2f2;
  border-radius: 20rpx;
  white-space: nowrap;
}
.search-history-del {
  font-size: 48rpx;
  color: #999;
}
